<?xml version="1.0" encoding="utf-8"?>
<!--suppress NavigationFile -->
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main_graph"
    app:startDestination="@id/splashFragment"
    >

    <fragment
        android:id="@+id/splashFragment"
        android:name="com.umc.presentation.ui.splash.SplashFragment"
        android:label="스플래쉬"
        tools:layout="@layout/fragment_splash">

        <action
            android:id="@+id/action_splash_to_home"
            app:destination="@id/nav_home"/>
    </fragment>

    <include
        android:id="@+id/nav_home"
        app:graph="@navigation/nav_home"/>

    <include
        android:id="@+id/nav_notice"
        app:graph="@navigation/nav_notice"/>

    <include
        android:id="@+id/nav_act"
        app:graph="@navigation/nav_act"/>

    <include
        android:id="@+id/nav_community"
        app:graph="@navigation/nav_community"/>

    <include
        android:id="@+id/nav_mypage"
        app:graph="@navigation/nav_mypage"/>

</navigation>